{
	"name": "rststore",
	"version": "1.0.0",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"start": "node server/main.js",
		"server": "nodemon server/main.js",
		"client": "npm run dev --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:import": "node server/seeder.js",
		"data:destroy": "node server/seeder.js -d"
	},
	"imports": {
		"#config/*": "./server/config/*",
		"#data/*": "./server/data/*",
		"#models/*": "./server/models/*",
		"#routes/*": "./server/routes/*",
		"#middlewares/*": "./server/middlewares/*",
		"#controllers/*": "./server/controllers/*"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@gitlab.com/rstforumnet/rststore-daily-evening-july-2025.git"
	},
	"keywords": [],
	"author": "Rahul Sharma",
	"license": "ISC",
	"bugs": {
		"url": "https://gitlab.com/rstforumnet/rststore-daily-evening-july-2025/issues"
	},
	"homepage": "https://gitlab.com/rstforumnet/rststore-daily-evening-july-2025#readme",
	"description": "",
	"dependencies": {
		"bcryptjs": "^3.0.2",
		"colors": "^1.4.0",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"mongoose": "^8.17.0",
		"morgan": "^1.10.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.28.0",
		"concurrently": "^9.2.0",
		"nodemon": "^3.1.10"
	}
}
